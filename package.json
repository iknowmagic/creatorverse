{
  "name": "creatorverse",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "check": "npm run typecheck && npm run lint && npm run format:check",
    "branch": "zsh scripts/newbranch.zsh",
    "snap": "zsh scripts/snapshot.zsh",
    "minor": "npm version minor",
    "major": "npm version major",
    "cosmos": "cosmos",
    "cosmos-export": "cosmos-export",
    "preview": "vite preview",
    "test": "vitest run --reporter=verbose",
    "test:watch": "vitest watch --reporter=verbose",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "test:ui": "vitest --ui --reporter=verbose",
    "test:file": "vitest --reporter=basic --reporter=json --outputFile=./test-output.json"
  },
  "dependencies": {
    "@react-router/node": "^7.6.2",
    "@react-router/serve": "^7.6.2",
    "@supabase/supabase-js": "^2.50.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-table": "^8.21.3",
    "framer-motion": "^12.18.1",
    "isbot": "^5.1.28",
    "lucide-react": "^0.522.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-router": "^7.6.2",
    "sonner": "^2.0.5",
    "swiper": "^11.2.8",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@react-router/dev": "^7.6.2",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.10",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.1",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "daisyui": "^5.0.43",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.2",
    "papaparse": "^5.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "react-cosmos": "^7.0.0",
    "react-cosmos-plugin-vite": "^7.0.0",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
